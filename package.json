{
  "name": "express-sequelize-api-template",
  "version": "1.0.0",
  "description": "Plantilla para crear APIs con Express.js, Sequelize y PostgreSQL",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server",
    "dev": "nodemon src/server",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "lint": "echo 'No linter configured' && exit 0",
    "test": "echo 'No tests configured' && exit 0"
  },
  "keywords": [
    "express",
    "sequelize",
    "postgresql",
    "api",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.0",
    "helmet": "8.0.0",
    "pg": "8.13.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.37.3"
  },
  "devDependencies": {
    "nodemon": "3.1.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jzaruma1209/create-exp-sq-paul.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jzaruma1209/create-exp-sq-paul/issues"
  },
  "homepage": "https://github.com/jzaruma1209/create-exp-sq-paul#readme"
}
